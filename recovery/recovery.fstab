/data           ext4        /dev/block/mmcblk0p7
/cache          ext4        /dev/block/mmcblk0p6
/boot           emmc        /dev/bootimg
/recovery       emmc        /dev/recovery
/storage        vfat        /dev/block/mmcblk0p8
/sdcard         vfat        /dev/block/mmcblk1p1
